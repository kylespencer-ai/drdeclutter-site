{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "redirects": [
    {
      "source": "/*",
      "destination": "https://www.drdeclutter.co.uk{path}",
      "redirectType": "301",
      "conditions": [
        {
          "name": "host",
          "match": "drdeclutter.co.uk",
          "operator": "equals"
        }
      ]
    }
  ]
}
