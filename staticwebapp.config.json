{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "redirects": [
    {
      "source": "/",
      "destination": "https://www.drdeclutter.co.uk/",
      "redirectType": "301",
      "condition": "Host == 'drdeclutter.co.uk'"
    },
    {
      "source": "/{*path}",
      "destination": "https://www.drdeclutter.co.uk/{path}",
      "redirectType": "301",
      "condition": "Host == 'drdeclutter.co.uk'"
    }
  ]
}
